.menu{

}
.menu  .menu-exit{
  height:100px;
  width:500px;
  position: absolute;
  bottom:30px;
  left:75px;
  background:#fff;
  font-size:34px;
  border-radius:50px;
  line-height:100px;
  text-align: center
}
.menu  .menu-exit img{
  vertical-align: middle;
}
.menu  .menu-middle-position{
  width:100%;
}
.menu .menu-list{
  position: fixed;
  top:0;
  left:-670px;
  width:650px;
  -webkit-transition: all .3s ease;
  -moz-transition: all .3s ease ;
  -ms-transition: all .3s ease ;
  -o-transition: all .3s ease ;
  transition: all .3s ease ;
  z-index: 999;
  background: #f5f5f5;
  height:100%;
}
.menu .show{
  -webkit-transform: translateX(670px);
  -moz-transform: translateX(670px);
  -ms-transform: translateX(670px);
  -o-transform: translateX(670px);
  transform: translateX(670px);
}
.menu .menu-top{
  position: relative;
  background: rgba(7, 17, 27, 0.5);
}
.menu .menu-top .background-mask{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height:278px;
  z-index: -1;
  -webkit-filter: blur(10px);
  filter: blur(10px);
}
.menu .menu-top .background-bg{
  width: 100%;
  height:278px;
  overflow: hidden;
  z-index: -1;
}
.menu .menu-top  .background-bg1{
  width: 100%;
  height:278px;
  overflow: hidden;
  z-index: -1;
  background: url("../../static/images/tx_pic.jpg") no-repeat;
  background-size:cover;
  background-position:center;
}
.menu .menu-top .background-bg img{
 /* width: 100%;
  height: 100%;*/
}
.menu .menu-top-setting{
  height:36px;
  text-align: right;
  line-height:36px;
  padding-right:20px;
}
.menu .menu-top-setting i{
  font-size:40px;
  color: #fff;
}
.menu .menu-top-user{
  padding:40px 0 80px 0;
}
.menu .menu-top-user ul{
  display: flex;
}
.menu .menu-top-user ul li:nth-of-type(1){
  flex:0 0 154px;
  text-align: center;
  position: relative;
}
.menu .menu-top-user ul li:nth-of-type(1) img{
  height:116px;
  width:116px;
  border-radius:50%;
}
.menu .menu-top-user ul li:nth-of-type(1) .bg-v{
  display: block;
  height: 30px;
  width: 30px;
  right: 10px;
  bottom: 10px;
  position: absolute;
  background: url("../../static/images/v.png") no-repeat;
  background-size :100% 100%;
}
.menu .menu-top-user ul li:nth-of-type(2){
  flex: 1;
  text-align: left;
}
.menu .menu-top-user ul li:nth-of-type(2) p:nth-of-type(1){
  font-size:28px;
  color: #fff;
  position: relative;
}
.menu .menu-top-user ul li:nth-of-type(2) p:nth-of-type(1) img{
  display : inline-block;
  width:30px;
  height:30px;
  margin-left:8px;
}
.menu .menu-top-user ul li:nth-of-type(2) p:nth-of-type(2){
  margin-top: 12px;
  font-size:22px;
  color: #fff;
}
.menu .menu-top-user ul li:nth-of-type(2) p:nth-of-type(3){
  margin-top: 10px;
  width:260px;
  background: #fff;
  height:16px;
  border-radius : 7px 7px;
  display: flex;
  overflow: hidden;
}
.menu .menu-top-user ul li:nth-of-type(2) p:nth-of-type(3) span{
  display: block;
  background: #41a3ff;
  border-radius : 7px 7px;
}
.menu .menu-top-user ul li:nth-of-type(3){
  line-height: 116px;
  flex:0 0 70px;
  text-align: center;
}
.menu .menu-top-user ul li:nth-of-type(3) i{
  font-size:30px;
  color: #fff;
}
.menu .menu-top-message ul{
  display: flex;
  display: -webkit-flex;
}
.menu .menu-top-message ul li{
  -webkit-box-flex: 1;
  flex: 1;
  text-align: center;
}
.menu .menu-top-message ul{
  padding:28px 0 40px 0;
}
.menu .menu-top-message ul li:nth-of-type(1) p:nth-of-type(1){
  font-size:30px;
  color: #fff;

}
.menu .menu-top-message ul li:nth-of-type(1) p:nth-of-type(2){
  padding-top: 10px;
  font-size:22px;
  color: #fff;
}
.menu .menu-top-message ul li:nth-of-type(2){
  display: flex;
  display: -webkit-flex;
}
.menu .menu-top-message ul li:nth-of-type(2) p:nth-of-type(1){
  margin-top: 5px;
  flex:0 0 1px;
  height:40px;
  background: #fff;
}
.menu .menu-top-message ul li:nth-of-type(2) p:nth-of-type(2){
  flex:1;
}
.menu .menu-top-message ul li:nth-of-type(2) p:nth-of-type(2) span{
  display: block;
}
.menu .menu-top-message ul li:nth-of-type(2) p:nth-of-type(2) span:nth-of-type(1){
  font-size:30px;
  color: #fff;
}
.menu .menu-top-message ul li:nth-of-type(2) p:nth-of-type(2) span:nth-of-type(2){
  padding-top: 10px;
  font-size:22px;
  color: #fff;
}
.menu .menu-top-message ul li:nth-of-type(2) p:nth-of-type(3){
  margin-top: 5px;
  flex:0 0 1px;
  height:40px;
  background: #fff;
}
.menu .menu-top-message ul li:nth-of-type(3) p:nth-of-type(1){
  font-size:30px;
  color: #fff;

}
.menu .menu-top-message ul li:nth-of-type(3) p:nth-of-type(2){
  padding-top: 10px;
  font-size:22px;
  color: #fff;
}
.menu .menu-wrapper{
  max-height: 600px;
  overflow: hidden;
}
.menu .menu-middle ul{
  display: flex;
  display: -webkit-flex;
  background: #fff;
  height:90px;
}
.menu .menu-middle li:nth-of-type(1){
  margin-top:25px;
  flex:0 0 98px;
  text-align: center;
}
.menu .menu-middle li:nth-of-type(1) svg{
  width:40px;
  height:40px;
}
.menu .menu-middle li:nth-of-type(2){
  line-height: 90px;
  flex:1;
  display: flex;
  display: -webkit-flex;
  border-bottom:1px solid #eeeeee;
}
.menu .menu-middle li:nth-of-type(2) p:nth-of-type(1){
  flex:0 0 150px;
  color: #373a41;
  font-size:28px;
}
.menu .menu-middle li:nth-of-type(2) p:nth-of-type(2){
  text-align: right;
  flex:1;
  color: #b1b1b1;
  font-size:24px;
}
.menu .menu-middle li:nth-of-type(2) p:nth-of-type(3){
  text-align: center;
  flex:0 0 60px;
  font-size:24px;
  color: #b1b1b1;
}
.menu .menu-middle li:nth-of-type(2) p:nth-of-type(3) i{
  font-size:28px;
}
.menu .menu-bottom {
  position: absolute;
  bottom:0;
  left:0;
  width:100%;
  z-index: 10;
}
.menu .menu-bottom ul{
  display: flex;
  display: -webkit-flex;
  background: #fff;
}
.menu .menu-bottom ul li{
  -webkit-box-flex: 1;
  flex: 1;
  text-align: center;
  padding-bottom:30px;
  padding-top:26px;
}
.menu .menu-bottom ul li p:nth-of-type(1) svg{
  height:50px;
  width:50px;
}
.menu .menu-bottom ul li p:nth-of-type(1) i{
  font-size:50px;
  color: #6f6f6f;
}
.menu .menu-bottom ul li p:nth-of-type(2){
  padding-top:12px;
  font-size:22px;
  color: #44474e;
}
