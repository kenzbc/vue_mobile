.started{
  margin: 100px 0 98px 0;
}
.started  .icon-size{
  vertical-align :middle;
  width: 22px;
  height: 22px;
  margin-bottom:6px;
}
.started .started-first{
  background: #fff;
  margin:0 20px 20px;
  border-radius: 8px;
  box-shadow: 5px 5px 8px rgba(0,200,215,0.1);
}
.started .started-first-title{
  height: 106px;
  background-size: 100% 100%;
}
.started .started-first-title-left{
  float: left;
  margin:16px 0 0 30px;
  width: 68px;
  height: 68px;
  line-height: 68px;
  border-radius:50%;
  font-size: 24px;
  color: #fff;
  text-align: center;
  font-weight:bold;
}
.started .started-first-title-middle{
  float: left;
  margin: 16px 0 0 16px;
}
.started .started-first-title-middle p{
  width:400px;
  overflow: hidden;
  white-space :nowrap;
  text-overflow :ellipsis;
}
.started .started-first-title-middle p:nth-of-type(1){
  font-size:28px;
}
.started .started-first-title-middle p:nth-of-type(2){
  margin-top:5px;
  font-size:22px;
  color: #999;
}
.started .started-first-title-right{
  float: right;
  margin: 16px 0 0 0;
}
.started .started-first-title-right .started-first-title-right-earn{
  float: left;
  margin-right: 35px;
}
  .started .started-first-title-right .started-first-title-right-arrow{
    float: left;
    margin: 15px 10px 0 0;
  }
.started .started-first-title-right .started-first-title-right-earn p:nth-of-type(1){
  text-align: right;
  font-size: 30px;
}
.started .started-first-title-right .started-first-title-right-earn p:nth-of-type(1) span{
  color: #0C9DFC;
  font-size: 30px;
}
.started .started-first-title-right .started-first-title-right-earn p:nth-of-type(2){
  font-size:20px;
  color: #999999;
}
.started .started-first-message{
  border-top: 1px solid #F1F1F1;
  margin: 0 20px;
  height:62px;
  line-height:62px;
  background: #fff;
}
.started .started-first-message li{
  float: right;
}
.started .started-first-message li:nth-of-type(1){
  padding-right:24px;
  font-size:24px;
}
.started .started-first-message li:nth-of-type(2){
  padding-right:8px;
}
.started .started-first-message li:nth-of-type(2) svg{
  height:30px;
  width:30px;
  vertical-align :middle;
  margin-bottom:4px;
}
.started .started-first-message li:nth-of-type(3){
  padding-right:20px;
  font-size:24px;
}
.started .started-first-message li:nth-of-type(4){
  padding-right:20px;
  font-size:24px;
}
.started .started-first-message li:nth-of-type(5){
  padding-right:8px;
  font-size:24px;
  text-align: center
}
.started .started-first-message li:nth-of-type(5) svg{
  height:24px;
  width:24px;
  vertical-align :middle;
  margin-bottom:4px;
}
.started .started-first-cont{
  height:90px;
  background: #fff;
  border-radius:0 0 10px 10px;
}
.started .started-first-cont ul{
  display: flex;
  display: -webkit-flex;
  width:100%;
}
.started .started-first-cont ul li{
  -webkit-box-flex: 1;
  flex: 1;
  text-align: center;
}
.started .started-first-cont ul li p:nth-of-type(2){
  color: #999999;
  font-size:24px;
}
.started .started-first-cont ul li p:nth-of-type(1){
  color: #373a41;
  font-size:30px;
  font-weight:bold;
}
.started .started-first-cont ul li:nth-of-type(3) p:nth-of-type(1){
  color: #ccba87;
  font-size:30px;
  font-weight:bold;
}
.started  .started-no-data{
  font-size:26px;
  text-align: center;
  height:60px;
  color:#999;
}
.started-null{
  position: fixed;
  top:50%;
  left:50%;
  margin-top:-100px;
  margin-left:-150px;
  width:300px;
}
.started-null img{
  margin-left: 50px;
  width:200px;
  height:200px;
}
.started-null p{
  text-align: center;
  font-size:28px;
  color: #999;
}
